{
  "name": "whitelabelnews",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:reset-cache": "react-native start --reset-cache",
    "android:daily-bugle": "cp env.DailyBugle.json env.json && react-native run-android --variant 'dailyBugleDebug' --appIdSuffix 'dailybugle'",
    "android:daily-planet": "cp env.DailyPlanet.json env.json && npx react-native run-android --variant 'dailyPlanetDebug' --appIdSuffix 'dailyplanet'",
    "ios:daily-bugle": "cp env.DailyBugle.json env.json && react-native run-ios --scheme 'DailyBugle'",
    "ios:daily-planet": "cp env.DailyPlanet.json env.json && react-native run-ios --scheme 'DailyPlanet'"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react": "~18.0.27",
    "axios": "^1.3.4",
    "expo": "~48.0.6",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-hook-form": "^7.43.5",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-modalize": "^2.1.1",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.2",
    "typescript": "^4.9.4",
    "whitelabelnews-state-manager": "^1.0.6",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/redux-logger": "^3.0.9",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-module-resolver-image": "^3.1.10",
    "react-native-svg-transformer": "^1.0.0"
  },
  "private": true
}
